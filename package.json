{
	"name": "argon-dashboard-react",
	"version": "1.2.0",
	"description": "React version of Argon Dashboard by Creative Tim",
	"main": "index.js",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/creativetimofficial/argon-dashboard-react.git"
	},
	"keywords": [
		"react",
		"reactjs",
		"argon",
		"argon-react",
		"dashboard",
		"dashboard-react",
		"argon-dashboard",
		"argon-dashboard-react"
	],
	"author": "Creative Tim",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/creativetimofficial/argon-dashboard-react/issues"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build && firebase deploy",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
		"compile:scss": "node-sass --importer node_modules/node-sass-package-importer/dist/cli.js src/assets/scss/argon-dashboard-react.scss src/assets/css/argon-dashboard-react.css",
		"minify:scss": "node-sass --importer node_modules/node-sass-package-importer/dist/cli.js src/assets/scss/argon-dashboard-react.scss src/assets/css/argon-dashboard-react.min.css --output-style compressed",
		"map:scss": "node-sass --importer node_modules/node-sass-package-importer/dist/cli.js src/assets/scss/argon-dashboard-react.scss src/assets/css/argon-dashboard-react.css --source-map true",
		"build:scss": "npm run compile:scss && npm run minify:scss && npm run map:scss"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	],
	"dependencies": {
		"@cyntler/react-doc-viewer": "^1.10.1",
		"@emotion/react": "^11.7.0",
		"@emotion/styled": "^11.6.0",
		"@fortawesome/fontawesome-free": "5.15.2",
		"@mui/material": "^5.2.3",
		"axios": "^0.27.2",
		"bootstrap": "4.6.0",
		"chart.js": "2.9.4",
		"classnames": "2.2.6",
		"copy-to-clipboard": "^3.3.1",
		"file-saver": "^2.0.5",
		"firebase": "^8.10.0",
		"moment": "2.29.1",
		"node-sass": "4.14.1",
		"node-sass-package-importer": "5.3.2",
		"nouislider": "14.6.3",
		"pdf-viewer-reactjs": "^2.2.3",
		"react": "17.0.1",
		"react-chartjs-2": "2.11.1",
		"react-copy-to-clipboard": "5.0.3",
		"react-datetime": "3.0.4",
		"react-device-detect": "^2.1.2",
		"react-doc-viewer": "^0.1.5",
		"react-dom": "17.0.1",
		"react-icons": "^4.12.0",
		"react-loading-overlay": "^1.0.1",
		"react-pdf": "^6.2.0",
		"react-qr-code": "^2.0.8",
		"react-redux": "^7.2.6",
		"react-router-dom": "5.2.0",
		"react-scripts": "4.0.1",
		"react-toastify": "^8.1.0",
		"reactstrap": "8.9.0",
		"redux": "^4.1.2",
		"redux-thunk": "^2.4.0"
	},
	"devDependencies": {
		"gulp": "4.0.2",
		"gulp-append-prepend": "1.0.8"
	},
	"optionalDependencies": {
		"eslint-plugin-flowtype": "5.2.0",
		"jquery": "3.5.1",
		"typescript": "4.1.3"
	}
}
